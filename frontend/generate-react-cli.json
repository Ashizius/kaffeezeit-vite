{
  "usesTypeScript": true,
  "usesStyledComponents": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": ".templates/TemplateComponent/TemplateName.tsx",
        "style": ".templates/TemplateComponent/TemplateName.module.scss",
        "story": ".templates/TemplateComponent/TemplateName.stories.tsx",
        "index": ".templates/TemplateComponent/index.tsx",
        "types": ".templates/TemplateComponent/types.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withIndex": true,
      "withTypes": true,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": ".templates/TemplatePage/TemplateName.tsx",
        "style": ".templates/TemplatePage/TemplateName.module.scss",
        "story": ".templates/TemplatePage/TemplateName.stories.tsx",
        "lazy": ".templates/TemplatePage/index.tsx",
        "types": ".templates/TemplatePage/types.ts"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": true,
      "withTypes": true
    }
  }
}